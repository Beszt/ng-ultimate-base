{
  "printWidth": 100,
  "trailingComma": "all",
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.component.html",
      "options": { "parser": "angular" }
    },
    {
      "files": "*.html",
      "options": { "parser": "html" }
    }
  ]
}
